<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="Kilometers_to_Miles" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="root_frame" named="True">
        <property name="height">200</property>
        <property name="padding">10</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="ipadx">0</property>
          <property name="pady">0</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">n</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="label10">
            <property name="font">{Arial} 16 {bold}</property>
            <property name="text" translatable="yes">Kilometers to Miles</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame7">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="ttk.Label" id="label11">
                <property name="font">{arial} 10 {}</property>
                <property name="text" translatable="yes">Kilometers: </property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="kilometer_entry" named="True">
                <property name="width">10</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="ipadx">5</property>
                  <property name="pady">5</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame8">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
            <child>
              <object class="ttk.Label" id="label12">
                <property name="font">{Arial} 12 {bold}</property>
                <property name="text" translatable="yes">Conversion is!</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="columnspan">2</property>
                  <property name="row">0</property>
                  <property name="rowspan">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="label13">
                <property name="font">{arial} 10 {}</property>
                <property name="text" translatable="yes">Miles: </property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="total_entry" named="True">
                <property name="justify">right</property>
                <property name="state">readonly</property>
                <property name="textvariable">string:miles_variable</property>
                <property name="width">10</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="padx">5</property>
                  <property name="pady">5</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button6" named="True">
            <property name="command" type="command" cbtype="simple">calculate_kilometers</property>
            <property name="text" translatable="yes">Calculate</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
